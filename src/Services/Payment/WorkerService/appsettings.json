{
  "SqlServerRetryingOptions": {
    "MaxRetryCount": 5,
    "MaxSecondsRetryDelay": 5,
    "ErrorNumbersToAdd": []
  },
  "EventStoreOptions": {
    "SnapshotInterval": 5
  },
  "Serilog": {
    "MinimumLevel": {
      "Override": {
        "Microsoft": "Warning",
        "System.Net.Http.HttpClient": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "Theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console"
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId"
    ]
  }
}
